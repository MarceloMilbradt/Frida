.isCollapse = !this.isCollapse;\r\n    },\r\n    collapse() {\r\n      this.isCollapse = true;\r\n    },\r\n    logout() {\r\n      this.$store.dispatch(\"logout\");\r\n    },\r\n  },\r\n  computed: {\r\n    routes() {\r\n      const logged = this.$store.getters.getLogged;\r\n      const [home, ...routes] = this.$router.options.routes;\r\n      return [\r\n        home,\r\n        ...routes.filter(\r\n          (r) => r.meta?.requiresAuth === logged && r.meta?.hideNavbar !== true\r\n        ),\r\n      ];\r\n    },\r\n  },\r\n};\r\n</script>\r\n<style scoped>\r\n@media (max-width: 800px) {\r\n  .el-menu-horizontal .menu-item-name {\r\n    display: none;\r\n  }\r\n}\r\n.rotate-icon i {\r\n  transition: all 0.5s;\r\n}\r\n\r\n.el-menu-vertical .right {\r\n  float: right;\r\n}\r\n.el-menu-full-height {\r\n  height: 100%;\r\n}\r\n.el-menu-vertical {\r\n  text-align: center;\r\n  position: fixed;\r\n  z-index: 99999;\r\n}\r\n.el-menu-vertical:not(.el-menu--collapse) {\r\n  width: 200px;\r\n  min-height: 400px;\r\n}\r\n</style>\r\n"]}]}