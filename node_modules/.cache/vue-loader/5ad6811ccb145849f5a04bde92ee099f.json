AAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC,CAAC;gBACvE,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;cAC7B,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;YACb,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;UACV,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;QACV,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;MACX,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;IACX,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;EAChB,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC","file":"E:/AppFrida/src/components/FormAjuda.vue","sourceRoot":"","sourcesContent":["<template>\r\n  <el-card class=\"box-card\">\r\n    <el-container>\r\n      <el-header style=\"text-align: center\" height=\"20px\">Precisamos de algumas informações suas para podermos te\r\n        ajudar!</el-header>\r\n      <el-divider></el-divider>\r\n      <el-main>\r\n        <el-form :model=\"form\" :label-position=\"'top'\" label-width=\"70px\" :rules=\"rules\" ref=\"form\">\r\n          <el-row>\r\n            <el-col :span=\"24\">\r\n              <TextField :fontSize=\"'20px'\" prop=\"nome\" label=\"Qual nome ou apelido você quer usar ?\" v-model=\"form.nome\" />\r\n            </el-col>\r\n          </el-row>\r\n\r\n          <el-row>\r\n            <el-col :span=\"24\">\r\n              <TextField v-model=\"form.contato\" label=\"Como podemos entrar em contato ?\" />\r\n            </el-col>\r\n          </el-row>\r\n\r\n          <el-row :gutter=\"20\">\r\n            <el-col :span=\"geo ? 19 : 17\">\r\n              <TextField v-model=\"form.endereco\" label=\"Onde podemos te encontrar ?\" />\r\n            </el-col>\r\n            <el-col :span=\"geo ? 4 : 6\">\r\n              <el-button v-if=\"!geo\" class=\"geo\" :type=\"!geo ? 'info' : 'success'\" icon=\"el-icon-location-outline\" @click=\"getGeo\" round>\r\n                <span class=\"geo-text\">Localização</span>\r\n              </el-button>\r\n              <CircleCheck v-if=\"geo\" color=\"green\" size=\"2\" style=\"width: 2.25em; height: 2.25em; margin-bottom: 1.25em\" />\r\n            </el-col>\r\n          </el-row>\r\n          <el-row>\r\n            <el-col :span=\"24\">\r\n              <el-button icon=\"el-icon-check\" @click=\"clickSubmit\" type=\"primary\" plain>\r\n                <span class=\"\">Enviar</span>\r\n              </el-button>\r\n            </el-col>\r\n          </el-row>\r\n        </el-form>\r\n      </el-main>\r\n    </el-container>\r\n  </el-card>\r\n</template>\r\n\r\n<script>\r\nimport TextField from \"./TextField.vue\";\r\nimport { CircleCheck } from \"@element-plus/icons\";\r\nimport * as geolocation from \"../controller/MapBoxs\";\r\nimport * as controller from \"../controller/ctlAjuda\";\r\n\r\nexport default {\r\n  components: {\r\n    TextField,\r\n    CircleCheck,\r\n  },\r\n  name: \"FormPessoa\",\r\n  props: {\r\n    text: String,\r\n    anonimo: Boolean,\r\n  },\r\n  data() {\r\n    return {\r\n      id: null,\r\n      form: {\r\n        nome: \"\",\r\n        endereco: \"\",\r\n        contato: \"\",\r\n        data: new Date(),\r\n        coords: {\r\n          latitude: \"\",\r\n          longitude: \"\",\r\n        },\r\n      },\r\n      geo: false,\r\n    };\r\n  },\r\n  computed: {\r\n    rules() {\r\n      return {\r\n        nome: [\r\n          {\r\n            required: !this.anonimo,\r\n            message: \"Informe ao menos seu nome!\",\r\n            trigger: \"blur\",\r\n          },\r\n        ],\r\n      };\r\n    },\r\n  },\r\n  async created() {\r\n    var id = this.$route.query.id;\r\n    if (id) {\r\n      var dados = await controller.bucarPorId(id);\r\n      this.form = dados;\r\n      this.id = id;\r\n    }\r\n  },\r\n  methods: {\r\n    clickSubmit() {\r\n      this.$emit('clickSubmit', this.id, this.form);\r\n    },\r\n    async setGeo({ latitude, longitude }) {\r\n      console.log(latitude, longitude);\r\n      const ends = await geolocation.getLocation(latitude, longitude);\r\n      const endereco = ends?.place_name ?? \"\";\r\n      this.form = { ...this.form, endereco, coords: { latitude, longitude } };\r\n      this.geo = true;\r\n    },\r\n    getGeo() {\r\n      return navigator.geolocation.getCurrentPosition(\r\n        (geo) => this.setGeo(geo.coords),\r\n        console.log,\r\n        {\r\n          enableHighAccuracy: true,\r\n        }\r\n      );\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\ni {\r\n  font-size: 2.5em;\r\n}\r\n.el-row {\r\n  align-items: flex-end;\r\n}\r\n.geo {\r\n  margin-bottom: 1.375em;\r\n}\r\n.box-card {\r\n  width: 37.5em;\r\n  margin: auto;\r\n}\r\n@media (max-width: 800px) {\r\n  .box-card {\r\n    width: 90vw;\r\n  }\r\n  .geo-text {\r\n    display: none;\r\n  }\r\n}\r\n</style>"]}]}