n size=\"mini\" type=\"danger\" @click=\"handleDelete(scope.row)\"\r\n          >Deletar</el-button\r\n        >\r\n      </template>\r\n    </el-table-column>\r\n  </el-table>\r\n</template>\r\n\r\n<script>\r\nimport * as controller from \"../controller/ctlUsuario\";\r\nexport default {\r\n  name: \"Home\",\r\n  components: {},\r\n  data() {\r\n    return {\r\n      search: \"\",\r\n      dados: [],\r\n    };\r\n  },\r\n  methods: {\r\n    async listarDados() {\r\n      this.dados = await controller.listarTodos();\r\n    },\r\n    handleAdd() {\r\n      this.$router.push({ path: \"CadastrarUsuario\" });\r\n    },\r\n    handleEdit(row) {\r\n      this.$router.push({ path: \"CadastrarUsuario/\"+row.id);\r\n    },\r\n    async handleDelete(row) {\r\n      controller.excluir(row.id).then(() => {\r\n        this.listarDados();\r\n      });\r\n    },\r\n  },\r\n  async created() {\r\n    this.listarDados();\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\np {\r\n  text-align: center;\r\n}\r\n</style>\r\n"]}]}