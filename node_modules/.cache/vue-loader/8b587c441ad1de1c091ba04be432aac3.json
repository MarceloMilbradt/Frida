tima.filiacao.mae\" :hit=\"item\" />\r\n                </div>\r\n                <div>\r\n                  <b>Agressor(a):</b>\r\n                  <ais-highlight attribute=\"agressor.nome\" :hit=\"item\" />\r\n                </div>\r\n              </div>\r\n            </el-card>\r\n          </template>\r\n        </ais-hits>\r\n\r\n      </ais-instant-search>\r\n\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport * as controller from \"../controller/ctlDenuncia\";\r\nimport { client } from \"../controller/Algolia\";\r\nexport default {\r\n  name: \"Home\",\r\n  components: {},\r\n  data() {\r\n    return {\r\n      search: \"\",\r\n      dados: [],\r\n      select: \"\",\r\n      input3: \"\",\r\n    };\r\n  },\r\n  computed: {\r\n    searchClient() {\r\n      return client;\r\n    },\r\n  },\r\n  methods: {\r\n    async listarDados() {\r\n      this.dados = await controller.listarTodos();\r\n    },\r\n    handleAdd() {\r\n      this.$router.push({ path: \"Denuncia\" });\r\n    },\r\n    handleEdit(row) {\r\n      this.$router.push({ name: \"Denúncia\", params: { id: row.id } });\r\n    },\r\n    async handleDelete(row) {\r\n      controller.excluir(row.id).then(() => {\r\n        this.listarDados();\r\n      });\r\n    },\r\n  },\r\n  async created() {\r\n    this.listarDados();\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\np {\r\n  text-align: center;\r\n}\r\n.box-card {\r\n  text-align: left;\r\n  margin: 0 0 0.25em 0;\r\n}\r\n</style>\r\n"]}]}