n type=\"success\" @click=\"handleAdd()\">Incluir</el-button>\r\n  </el-row>\r\n  <el-table\r\n    :data=\"\r\n      this.dados.filter(\r\n        (data) =>\r\n          !search || data.vitima.nome.toLowerCase().includes(search.toLowerCase())\r\n      )\r\n    \"\r\n    border\r\n    style=\"width: 100%\"\r\n  >\r\n    <el-table-column label=\"Nome\" prop=\"vitima.nome\"> </el-table-column>\r\n    <el-table-column label=\"Telefone\" prop=\"vitima.telefone\"> </el-table-column>\r\n    <el-table-column label=\"Agressor\" prop=\"agressor.nome\"> </el-table-column>\r\n    <el-table-column align=\"right\">\r\n      <template #header>\r\n        <el-input\r\n          v-model=\"search\"\r\n          size=\"mini\"\r\n          placeholder=\"Pesquisar por nome...\"\r\n        />\r\n      </template>\r\n      <template #default=\"scope\">\r\n        <el-button size=\"mini\" @click=\"handleEdit(scope.row)\">Editar</el-button>\r\n        <el-button size=\"mini\" type=\"danger\" @click=\"handleDelete(scope.row)\"\r\n          >Deletar</el-button\r\n        >\r\n      </template>\r\n    </el-table-column>\r\n  </el-table> -->\r\n  <div>\r\n    <el-button type=\"success\" @click=\"handleAdd()\">Incluir</el-button>\r\n    <div style=\"margin-top: 15px;\">\r\n      <ais-instant-search :search-client=\"searchClient\" index-name=\"frida_denuncias\">\r\n        <ais-search-box>\r\n          <template v-slot=\"{ currentRefinement, refine }\">\r\n            <el-input type=\"search\" :value=\"currentRefinement\" @input=\"refine(input3)\" placeholder=\"Pesquisar\" v-model=\"input3\" class=\"input-with-select\">\r\n              <template #append>\r\n                <el-button icon=\"el-icon-search\"></el-button>\r\n              </template>\r\n            </el-input>\r\n          </template>\r\n        </ais-search-box>\r\n        <ais-hits>\r\n          <template v-slot=\"{ items }\">\r\n            <el-card class=\"box-card\" v-for=\"hit in items\" :key=\"hit.objectID\">\r\n\r\n              <el-descriptions :title=\"hit.filiacao?.mae ?? hit.vitima.nome +' - '+ new Date(hit.data).toLocaleDateString()\" column=\"3\" size=\"small\" border>\r\n\r\n                <template #extra>\r\n                  <el-button type=\"primary\" size=\"small\"></el-button>\r\n                </template>\r\n\r\n                <el-descriptions-item label-class-name=\"label-fixed\" :span=\"hit.vitima.cpf ? 2 : 3\">\r\n                  <template #label>\r\n                    <i class=\"el-icon-user\"></i>\r\n                    Vítima\r\n                  </template>\r\n                  <ais-highlight attribute=\"vitima.nome\" :hit=\"hit\" />\r\n                </el-descriptions-item>\r\n\r\n                <el-descriptions-item v-if=\"hit.vitima.cpf\" span=\"1\">\r\n                  <template #label>\r\n                    CPF\r\n                  </template>\r\n                  <ais-highlight attribute=\"vitima.cpf\" :hit=\"hit\" />\r\n                </el-descriptions-item>\r\n\r\n                <el-descriptions-item :span=\"hit.agressor.cpf ? 2 : 3\">\r\n                  <template #label>\r\n                    <i class=\"el-icon-user\"></i>\r\n                    Agressor(a)\r\n                  </template>\r\n                  <ais-highlight attribute=\"agressor.nome\" :hit=\"hit\" />\r\n                </el-descriptions-item>\r\n\r\n                <el-descriptions-item v-if=\"hit.agressor.cpf\" span=\"1\">\r\n                  <template #label>\r\n                    <i class=\"el-icon-user\"></i>\r\n                    CPF\r\n                  </template>\r\n                  <ais-highlight attribute=\"agressor.cpf\" :hit=\"hit\" />\r\n                </el-descriptions-item>\r\n\r\n                <el-descriptions-item span=\"3\">\r\n                  <template #label>\r\n                    <i class=\"el-icon-user\"></i>\r\n                    Mãe da Vítima\r\n                  </template>\r\n                  <ais-highlight attribute=\"vitima.filiacao.mae\" :hit=\"hit\" />\r\n                </el-descriptions-item>\r\n\r\n                <el-descriptions-item span=\"3\" v-if=\"hit.data\">\r\n                  <template #label>\r\n                    <i class=\"el-icon-tickets\"></i>\r\n                    Data\r\n                  </template>\r\n                   {{new Date(hit.data).toLocaleDateString()}}\r\n                </el-descriptions-item>\r\n\r\n                \r\n              </el-descriptions>\r\n            </el-card>\r\n          </template>\r\n        </ais-hits>\r\n\r\n      </ais-instant-search>\r\n\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport * as controller from \"../controller/ctlDenuncia\";\r\nimport { client } from \"../controller/Algolia\";\r\nexport default {\r\n  name: \"Home\",\r\n  components: {},\r\n  data() {\r\n    return {\r\n      search: \"\",\r\n      dados: [],\r\n      select: \"\",\r\n      input3: \"\",\r\n    };\r\n  },\r\n  computed: {\r\n    searchClient() {\r\n      return client;\r\n    },\r\n  },\r\n  methods: {\r\n    async listarDados() {\r\n      this.dados = await controller.listarTodos();\r\n    },\r\n    handleAdd() {\r\n      this.$router.push({ path: \"Denuncia\" });\r\n    },\r\n    handleEdit(row) {\r\n      this.$router.push({ name: \"Denúncia\", params: { id: row.id } });\r\n    },\r\n    async handleDelete(row) {\r\n      controller.excluir(row.id).then(() => {\r\n        this.listarDados();\r\n      });\r\n    },\r\n  },\r\n  async created() {\r\n    this.listarDados();\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\np {\r\n  text-align: center;\r\n}\r\n.box-card {\r\n  text-align: left;\r\n  margin: 1.25em;\r\n}\r\n.box-card:hover{\r\n  transform: scale(1.04);\r\n}\r\n.label-fixed{\r\n  max-width: 20px;\r\n  color: green;\r\n}\r\n</style>\r\n"]}]}